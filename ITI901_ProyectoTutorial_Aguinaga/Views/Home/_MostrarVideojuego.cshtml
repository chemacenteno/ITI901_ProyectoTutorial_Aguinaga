@model ITI901_ProyectoTutorial_Aguinaga.Models.Videojuego

<div class="panel panel-default">
	<div class="panel-heading">
		<h3 class="panel-title">
			@Model.Nombre
		</h3>
	</div>
	<div class="panel-body">
		<h4>Imagen</h4>
		<img class="img-responsive" src="@Model.ImagenUrl" />
		<hr />
		<h4>Descripción</h4>
		<p>
			@Model.Descripcion
		</p>
		<p><b>Género:</b> @Model.Genero.Nombre</p>
	</div>
</div>
